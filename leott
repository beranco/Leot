x=float(input('Enter a real number:'))
        
y=round(x)
if x>0:

    if y>x:
            intgP=y-1
    else:
            intgP=y
    print('Integer portion of entered number is: ', intgP)
    
else:
    if y<x:
        intgP=y+1
    else:
        intgP=y
    print('Integer portion of entered number is: ', intgP)

if intgP%2==0:
    print('Even')
else:
    print('Odd')

    
    
    
